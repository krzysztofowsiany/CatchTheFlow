<Window x:Class="CatchTheFlow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" >
    <StackPanel>
        <Button Height="20" Click="ButtonBase_OnClick">Start</Button>
        <Button Height="20" Click="ButtonStartShortBreake_OnClick">Start ShortBreake</Button>
        <Button Height="20" Click="ButtonStartLongBreake_OnClick">Start LongBreake</Button>
        <Button Height="20" Click="ButtonConfig_OnClick">Config</Button>
        <Button Height="20" Click="ButtonConfig2_OnClick">Config2</Button>
        <Button Height="20" Click="ButtonInterruptWork_OnClick">InterruptWork</Button>
        <Button Height="20" Click="ButtonInterruptShortBreak_OnClick">InterruptShortBreak</Button>
        <Button Height="20" Click="ButtonInterruptLongBreak_OnClick">InterruptLongBreak</Button>

    </StackPanel>
</Window>
